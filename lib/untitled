var gpio = require('gpio');
var gpioArr = [];


var allPins = [3, 5, 7, 8, 10, 11, 12, 13, 15, 16, 18, 19, 21, 22, 23, 24, 26, 29, 31, 32, 33, 35, 36, 37, 38, 40];

  allPins.forEach(function (el, index, array) {

    gpioArr[el] = gpio.export(el, {
      direction: 'out',
      interval: 10000,
      ready: function() {
        console.log('Ready ');
      }
    });
  });